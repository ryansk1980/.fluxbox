[begin] (Fluxbox)
 [submenu] (Applications)

  [submenu] (Terminals)
   [exec] (Xterm) {xterm -rv +sb -fn fixed}
  [end]

  [submenu] (Browsers)
   [exec] (Firefox) {firefox -width 1280 -height 1024}
   [exec] (Lynx) {xterm -rv +sb -fn fixed -e lynx}
  [end]

  [submenu] (Mail)
   [exec] (Mutt) {xterm -rv +sb -fn fixed -e mutt}
  [end]

  [submenu] (Messaging)
